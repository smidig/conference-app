= form_for @feedback_vote do |f|
  - if @feedback_vote.errors.any?
    #error_explanation
      %h2= "#{pluralize(@feedback_vote.errors.count, "error")} prohibited this feedback_vote from being saved:"
      %ul
        - @feedback_vote.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :comment
    = f.text_field :comment
  .field
    = f.label :vote
    = f.number_field :vote
  .actions
    = f.submit 'Save'
