.row-fluid.program-admin
  .span9
    %h1 Tidsslotter:
    - @timeslots.each do |timeslot|
      = render 'timeslot', :timeslot => timeslot

    %p
      = link_to 'Legg til timeslot', new_timeslot_path


  .span3
    .available-talks-container
      %h2 Godkjente talks:
      %ul.available-talks.talks
        - for talk in @available_talks
          = render 'talk', :talk => talk